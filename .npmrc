# NPM Configuration for better performance

# Enable package-lock for consistent installs
package-lock=true

# Use faster registry
registry=https://registry.npmjs.org/

# Reduce concurrent installations
maxsockets=5

# Enable progress reporting
progress=true

# Skip unnecessary scripts during install
ignore-scripts=false

# Prefer offline mode for faster installs
prefer-offline=true

# Handle peer dependency conflicts
legacy-peer-deps=true

