{
  "name": "reride-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "A modern vehicle marketplace platform.",
  "scripts": {
    "dev": "vite",
    "dev:clean": "npm run clean && vite",
    "dev:fast": "npm run clean && vite --force",
    "build": "vite build",
    "build:analyze": "npm run build && node performance-optimize.js --analyze",
    "preview": "vite preview",
    "seed": "node seed-simple.js",
    "clean": "node -e \"const fs = require('fs'); const path = require('path'); const dirs = ['node_modules/.vite', 'dist']; dirs.forEach(d => { try { fs.rmSync(d, { recursive: true, force: true }); console.log('Cleaned:', d); } catch(e) {} });\"",
    "perf": "node performance-optimize.js",
    "perf:clean": "node performance-optimize.js --clean",
    "perf:analyze": "node performance-optimize.js --analyze",
    "verify-deployment": "node scripts/verify-deployment.js",
    "verify-deployment:local": "node scripts/verify-deployment.js http://localhost:5173",
    "test-categories": "node scripts/test-category-sync.js",
    "test-categories:local": "node scripts/test-category-sync.js http://localhost:5173",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false"
  },
  "dependencies": {
    "@google/generative-ai": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "chart.js": "^4.5.0",
    "cors": "^2.8.5",
    "dompurify": "^3.0.8",
    "express": "^5.1.0",
    "express-rate-limit": "^7.1.5",
    "firebase": "^10.7.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.3",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1",
    "react-window": "^1.8.8",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/dompurify": "^3.0.5",
    "@types/jest": "^29.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.14.2",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/react-window": "^1.8.8",
    "@types/validator": "^13.11.7",
    "@vercel/node": "^5.3.24",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.0.0",
    "node-mocks-http": "^1.17.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "ts-jest": "^29.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.3.1",
    "vite-plugin-pwa": "^1.1.0",
    "workbox-window": "^7.3.0"
  }
}
