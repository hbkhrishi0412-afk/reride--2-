{
  "name": "reride-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "A modern vehicle marketplace platform.",
  "scripts": {
    "dev": "vite",
    "dev:clean": "npm run clean && vite",
    "build": "vite build",
    "preview": "vite preview",
    "seed": "node seed-simple.js",
    "clean": "node -e \"const fs = require('fs'); const path = require('path'); const dirs = ['node_modules/.vite', 'dist']; dirs.forEach(d => { try { fs.rmSync(d, { recursive: true, force: true }); console.log('Cleaned:', d); } catch(e) {} });\""
  },
  "dependencies": {
    "@google/genai": "^1.17.0",
    "chart.js": "^4.5.0",
    "firebase": "^10.7.1",
    "react": "^19.1.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vercel/node": "^5.3.24",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.4.5",
    "vite": "^5.3.1"
  }
